{
    "version": 2,
    "functions": {
        "api/*.js": {
            "runtime": "vercel-edge@2.0.0"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}